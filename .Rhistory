rpartXse=vector("numeric",length=nreps))
n <- nrow(modelDat)
set.seed(1234)
for(i in 1:nreps) {
# random sample with replacement
sp <- sample(n,n,replace=TRUE)
# data splitting
tr <- modelDat[sp,]
ts <- modelDat[-sp,]
# model learning and prediction
m <- nnet(N ~ Beat + HourType,tr, size=8, decay=0.1, maxit=1000)
p <- predict(m,ts)
# evaluation
